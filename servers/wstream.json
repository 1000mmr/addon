{
    "id": "wstream",
    "name": "Wstream",
    "active": true,
    "free": true,
    
    "find_videos": {
        "patterns": [
            {
                "pattern": "(?:wstream\\.video|wstream\\.icu)(?!<)(?:=|/)(?:video[a-zA-Z0-9.?_]*|embed[a-zA-Z0-9]*|)?(?!api|swembedid)(?:-|/|=)?(?:[a-z0-9A-Z]+/)?([a-z0-9A-Z]+)",
                "url": "http://wstream.video/video.php?file_code=\\1"
            },
            {
                "pattern": "(?:wstream\\.video|wstream\\.icu)/((?:api/vcmod/fastredirect/streaming\\.php\\?id=|swembedid/)[$0-9]+)",
                "url": "http://wstream.video/\\1"
            },
            {
                "pattern": "https://nored.icu/swembedid/([0-9]+)",
                "url": "http://nored.icu/swembedid/\\1"
            }
        ],
        "ignore_urls": []
    },
    "settings": [
        {
            "default": false,
            "enabled": true,
            "id": "black_list",
            "label": "$ADDON[plugin.video.kod 70708]",
            "type": "bool",
            "visible": true
        },
        {
            "default": 0,
            "enabled": true,
            "id": "favorites_servers_list",
            "label": "$ADDON[plugin.video.kod 60655]",
            "lvalues": [
                "No",
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "type": "list",
            "visible": false
        }
    ],
    "cloudflare": true
}
